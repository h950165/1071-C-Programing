#include<stdio.h>

int main()
{
  int h1,h2,h3;
  int k1,k2;
  printf("Enter the height (-1 to exit): ");
  scanf("%d %d",&h1,&h2);

  if(h1=-1)
    {
    printf("Bye! Coding by 407411312");
    }

  for(int i=h1;i<=h2;i++)
  {

    k1=18.5*h1*h1/10000;
    k2=24*h1*h1/10000;

    printf("%d %d~%d(kg)\n",i,k1,k2);
  }


}

